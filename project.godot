; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="FallingRhythm"
run/main_scene="res://script/level.tscn"
config/features=PackedStringArray("4.0", "Mobile")
config/icon="res://asset/sprite/icon/icon.svg"

[autoload]

EventStorage="*res://script/event_storage.gd"

[display]

window/size/viewport_width=1000
window/size/viewport_height=2000
window/handheld/orientation=1

[input_devices]

pointing/emulate_touch_from_mouse=true

[rendering]

renderer/rendering_method="mobile"
